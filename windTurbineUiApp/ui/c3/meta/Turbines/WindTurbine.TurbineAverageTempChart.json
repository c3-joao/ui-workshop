
{
    "type": "UiSdlConnected<UiSdlTimeseriesLineBarChart>",
    "component": {
      "header": {
        "title": "Average Gear Oil Temperature"
      },
      "legend": {
        "legendPosition": "BELOW_CHART"
      },
  
      "zoomEnabled": true,
      "dataZoom": true,
      "dataSelection": true,
      "xAxis": {
        "type": "UiSdlTimeseriesLineBarChartXAxisConfig",
        "name": "Gear Oil Temperature"
      },
      "yAxis": {
        "name": "Power",
        "units": true
      },
      "dataSpec": {
        "dataType": "WindTurbine",
        "contextVars": {
          "id": {
            "type": "UiSdlApplicationStateValueParam",
            "id": "WindTurbine.UiSdlApplicationStateWT",
            "path": "pageId"
          }
        },
        "yAxisFields": [
          {
            "visualizationType": {
              "type": "UiSdlTimeseriesLineBarChartLineVisualization",
              "lineStyle": "Solid",
              "itemStyle": "CIRCLE"
            },
  
            "entityId": "${id}",
            "metricName": "gearOilTemperature",
            "bindings": {
              "value": {
                "origin": 0,
                "bound": 100
              }
            }
          }
        ]
      }
    }
  }
  