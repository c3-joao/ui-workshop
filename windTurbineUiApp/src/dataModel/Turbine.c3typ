entity type Turbine schema name 'TRBN' {
    name: string
    latitude: float
    longitude: float
    operationalStatus: string enum ('Operational', 'Decommissioned')
    isInMaintenance: boolean
    model: Model
    workOrders: [WorkOrder](windturbine)
    events: [TurbineEvent](windturbine)
    latestEvent: EventType stored calc latestValue(events.eventType)
}
